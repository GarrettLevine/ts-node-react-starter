{
  "name": "ts-node-react",
  "version": "1.0.0",
  "description": "Smal Typescript node project with react",
  "main": "index.js",
  "scripts": {
    "start": "npm run clean && npm run build && npm run watch",
    "build": "npm run build-ts && npm run tslint",
    "build-react": "./node_modules/.bin/webpack-dev-server --config webpack.config.dev.js",
    "build-ts": "tsc",
    "clean": "rm -rf public && mkdir -p public",
    "serve": "./node_modules/.bin/nodemon public/lib/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve\" \"npm run build-react\"",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "watch-ts": "tsc -w",
    "build-prod": "npm run clean && npm run build-prod-fe && npm run build-ts && node ./public/lib/server.js",
    "build-prod-fe": "./node_modules/.bin/webpack --config webpack.config.prod.js"
  },
  "nodemonConfig": {
    "delay": 2500
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "bulma": "^0.7.1",
    "classnames": "^2.2.6",
    "express": "^4.16.1",
    "history": "^4.7.2",
    "pg": "^7.4.3",
    "react": "^16.8.6",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.8",
    "@types/classnames": "^2.2.4",
    "@types/express": "^4.0.39",
    "@types/history": "^4.6.2",
    "@types/pg": "^7.4.10",
    "@types/react": "^16.0.28",
    "@types/react-dom": "^16.0.3",
    "@types/react-redux": "^5.0.14",
    "@types/react-router-dom": "^4.2.3",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^6.26.3",
    "babel-loader": "7",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^3.5.0",
    "css-loader": "^2.1.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.5.3",
    "nodemon": "^1.12.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.4.4",
    "ts-node": "^3.3.0",
    "tslint": "^5.7.0",
    "typescript": "^3.4.3",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.3.1",
    "write-file-webpack-plugin": "^4.5.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
