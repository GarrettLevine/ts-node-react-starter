{
  "name": "ts-node-react",
  "version": "1.0.0",
  "description": "Smal Typescript node project with react",
  "main": "index.js",
  "scripts": {
    "start": "npm run clean && npm run build && npm run watch",
    "build": "npm run build-ts && npm run tslint",
    "build-react": "./node_modules/.bin/webpack-dev-server",
    "build-ts": "tsc",
    "clean": "rm -rf public && mkdir -p public",
    "serve": "./node_modules/.bin/nodemon public/lib/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve\" \"npm run build-react\"",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "watch-ts": "tsc -w"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "express": "^4.16.1",
    "history": "^4.7.2",
    "react": "^16.0.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.8",
    "@types/express": "^4.0.39",
    "@types/history": "^4.6.2",
    "@types/react": "^16.0.28",
    "@types/react-dom": "^16.0.3",
    "@types/react-redux": "^5.0.14",
    "@types/react-router-dom": "^4.2.3",
    "@types/redux": "^3.6.0",
    "awesome-typescript-loader": "^3.2.3",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^3.5.0",
    "css-loader": "^0.28.7",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.5.3",
    "nodemon": "^1.12.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "ts-loader": "^2.3.7",
    "ts-node": "^3.3.0",
    "tslint": "^5.7.0",
    "typescript": "^2.5.3",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1",
    "write-file-webpack-plugin": "^4.2.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
